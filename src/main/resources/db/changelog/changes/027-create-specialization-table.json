{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "027-create-specialization-table",
        "author": "goodmood",
         "validCheckSum": "ANY",
        "comment": "Create specialization-maptable",
        "changes": [
          {
            "createTable": {
              "tableName": "specializations",
              "columns": [
                {
                  "column": {
                    "name": "id",
                    "type": "bigint",
                    "autoIncrement": true,
                    "constraints": {
                      "primaryKey": true,
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "name",
                    "type": "VARCHAR(255)"
                  }
                },
                {
                  "column": {
                    "name": "des",
                    "type": "LONGTEXT",
                     "constraints": {
                      "nullable": true
                    }
                  }
                },
                {
                  "column": {
                    "name": "status",
                    "type": "int",
                    "defaultValueNumeric": 1
                  }
                },
                {
                  "column": {
                    "name": "created_at",
                    "type": "datetime",
                    "defaultValueComputed": "CURRENT_TIMESTAMP",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "updated_at",
                    "type": "datetime",
                    "defaultValueComputed": "CURRENT_TIMESTAMP",
                    "constraints": {
                      "nullable": false
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}