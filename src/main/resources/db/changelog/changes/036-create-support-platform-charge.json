{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "036-create-support-platform-charge",
        "author": "goodmood",
        "comment": "Create support-platform-charge table",
        "changes": [
          {
            "createTable": {
              "tableName": "support-platform-charges",
              "columns": [
                {
                  "column": {
                    "name": "id",
                    "type": "bigint",
                    "autoIncrement": true,
                    "constraints": {
                      "primaryKey": true,
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "suport_category",
                    "type": "bigint",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                  {
                  "column": {
                    "name": "charge",
                    "type": "int",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "status",
                    "type": "int",
                    "defaultValueNumeric": 1,
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "created_at",
                    "type": "datetime",
                    "defaultValueComputed": "CURRENT_TIMESTAMP",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "updated_at",
                    "type": "datetime",
                    "defaultValueComputed": "CURRENT_TIMESTAMP",
                    "constraints": {
                      "nullable": false
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "changeSet": {
        "id": "035-seed-contract-type-table",
