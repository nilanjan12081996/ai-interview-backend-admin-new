                    "type": "varchar(255)",
                    "constraints": {        
                        "nullable": true
                    }
                  }
                },
                 {
                  "column": {
                    "name": "description",
                    "type": "longtext",
                     "constraints": {        
                        "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "color_code",
